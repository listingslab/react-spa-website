{
  "name": "react-spa-website",
  "description": "Listingslab's React SPA Website Seed.",
  "version": "2.0.0",
  "author": "Chris Dorward <listingslab@gmail.com> (http://listingslab.com)",
  "main": "start.js",
  "scripts": {
    "start": "node --use_strict start.js",
    "build": "clear && webpack --optimize-minimize && gulp --silent",
    "dev": "clear && webpack-dev-server --port 1976 --inline --progress --hot --content-base public --history-api-fallback",
    "lint": "eslint . --ext .js --ext .jsx || true"
  },
  "dependencies": {
    "bootstrap": "^3.3.6",
    "jquery": "^2.2.3",
    "react": "^15.0.1",
    "react-addons-css-transition-group": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-router": "^2.0.1"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "clear": "0.0.1",
    "colors": "^1.1.2",
    "css-loader": "^0.23.1",
    "del": "^2.2.0",
    "eslint": "^2.7.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.3.0",
    "gulp": "^3.9.1",
    "jshint": "^2.9.1",
    "jshint-loader": "^0.8.3",
    "jsx-loader": "^0.13.2",
    "run-sequence": "^1.1.5",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "repository": {
    "type": "git",
